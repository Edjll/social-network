{
  "defaultMessage": {
    "notNull": "must not be null",
    "minLength": "must be more than",
    "maxLength": "must be less than",
    "pattern": "is not correct"
  },
  "user": {
    "username": {
      "params": {
        "notNull": true,
        "minLength": 3,
        "maxLength": 15,
        "pattern": "^\\w+$"
      }
    },
    "email": {
      "params": {
        "notNull": true,
        "minLength": 9,
        "maxLength": 30,
        "pattern": "\\w+@\\w+\\.\\w+"
      }
    },
    "firstName": {
      "params": {
        "notNull": true,
        "minLength": 1,
        "maxLength": 15,
        "pattern": "^[a-zA-Zа-яА-Я]+$"
      }
    },
    "lastName": {
      "params": {
        "notNull": true,
        "minLength": 1,
        "maxLength": 15,
        "pattern": "^[a-zA-Zа-яА-Я]+$"
      }
    },
    "password": {
      "params": {
        "notNull": true,
        "minLength": 3,
        "maxLength": 15
      }
    }
  },
  "group": {
    "title": {
      "params": {
        "notNull": true,
        "minLength": 5,
        "maxLength": 40,
        "pattern": "[a-zA-Zа-яА-Я0-9_\\- ]"
      }
    },
    "description": {
      "params": {
        "notNull": true,
        "minLength": 10,
        "maxLength": 150
      }
    },
    "address": {
      "params": {
        "notNull": true,
        "minLength": 3,
        "maxLength": 15,
        "pattern": "^[a-zA-Z0-9_]+$"
      }
    }
  },
  "post": {
    "text": {
      "params": {
        "notNull": true,
        "maxLength": 100
      }
    }
  },
  "city": {
    "title": {
      "params": {
        "notNull": true,
        "minLength": 1,
        "maxLength": 20,
        "pattern": "^[a-zA-Zа-яА-Я]+ {0,1}[a-zA-Zа-яА-Я]+$"
      }
    },
    "country": {
      "params": {
        "notNull": true
      }
    }
  },
  "country": {
    "title": {
      "params": {
        "notNull": true,
        "minLength": 1,
        "maxLength": 20,
        "pattern": "^[a-zA-Zа-яА-Я]+ {0,1}[a-zA-Zа-яА-Я]+$"
      }
    }
  },
  "message": {
    "text": {
      "params": {
        "notNull": true,
        "minLength": 1,
        "maxLength": 150
      }
    }
  }
}